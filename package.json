{
  "name": "personal-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "server": "nodemon ./src/server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build:client": "vite build",
    "build:server": "cp -r ./src/server ./dist/",
    "build": "npm run build:client && npm run build:server",
    "serve": "screen -L -d -m -S server node ./dist/server/server.js && echo 'server started'",
    "test": "DOTENV_CONFIG_PATH=test.env cross-env NODE_OPTIONS=--experimental-vm-modules jest --setupFiles=dotenv/config --forceExit"
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.3.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "mocha": "^10.0.0",
    "mongodb": "^4.9.1",
    "mongoose": "^6.7.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "request": "^2.88.2",
    "showdown": "^2.1.0",
    "slugify": "^1.6.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "sass": "^1.49.9",
    "supertest": "^6.2.4",
    "vite": "^2.8.0"
  }
}